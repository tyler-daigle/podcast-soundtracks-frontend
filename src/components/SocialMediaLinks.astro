---
import type { PodcastSocialLinks } from "../lib/getstrapi";

interface Props {
  socialLinks: PodcastSocialLinks;
}
import SocialLink from "./SocialLink.astro";

const { socialLinks } = Astro.props;
---

<ul class="flex w-1/2 m-auto justify-evenly">
  {
    Array.from(socialLinks).map(([socialName, url]) =>
      url !== null ? (
        <li>
          <SocialLink socialName={socialName} url={url} />
        </li>
      ) : (
        ""
      )
    )
  }
</ul>
